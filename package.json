{
  "name": "tedir-select",
  "version": "0.0.1-beta-17",
  "description": "Tedir Select is a Vue 3 & Web Component Custom SelectBox, ListBox, Taggable, etc.",
  "main": "dist/vue/tedir-select-vue.umd.js",
  "module": "dist/vue/tedir-select-vue.es.js",
  "unpkg": "dist/vue/tedir-select-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue/tedir-select-vue.es.js",
      "require": "./dist/vue/tedir-select-vue.umd.js"
    },
    "./web": {
      "import": "./dist/web/tedir-select-web.es.js",
      "require": "./dist/web/tedir-select-web.umd.js"
    },
    "./css": "./dist/vue/style.css"
  },
  "scripts": {
    "dev-vue": "vite --config vite-vue-config.ts",
    "build-vue": "vue-tsc --noEmit && vite build --config vite-vue-config.ts",
    "build-web": "vue-tsc --noEmit && vite build --config vite-web-config.ts",
    "preview-vue": "vite preview --config vite-vue-config.ts"
  },
  "files": [
    "src/alga/*.alga",
    "package.alga",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedirghazali/select.git"
  },
  "keywords": [
    "selectbox",
    "listbox",
    "custom-select",
    "taggable"
  ],
  "author": "Tedir Ghazali",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/tedirghazali/select/issues"
  },
  "homepage": "https://github.com/tedirghazali/select#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "alga-css": "^1.0.0-beta-48",
    "alga-js": "^0.1.0-beta-14",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vue": "^3.2.39",
    "vue-tsc": "^2.2.2"
  }
}
